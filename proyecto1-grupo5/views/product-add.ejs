<%- include('./partials/head') %>
<%- include('./partials/headerLogueado') %>

 <!-- Novedades -->
 <div class="container products-wrapper">
    <div class="row">
        <div class="col-12">
            <h2 class="products-title">Agregar productos</h2>
        </div>
        <!-- Show the "ultimos" products -->
        <h2 class="col-12"> 
            <form action="/product/add" method="post">
                
                <div><input type="hidden" placeholder="Imagen" name="usuario_id" value="<%= locals.user.id %>"> </div>
                <div><input type="text" placeholder="Imagen" name="img"> </div>
                <div><input type="text" placeholder="Nombre del producto:" name="nombre"> </div>
                <div><input type="text" placeholder="Descripcion:" name="descripcion"> </div>
                <button type="submit" class="verdaro">Agregar Producto</button>
            </form>
        </h2>

<%- include('./partials/footer') %>